<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="Home" prefHeight="309.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testapp.FirstScreenController">
    <MenuBar>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem fx:id="info_help" mnemonicParsing="false" text="About" />
        </Menu>
    </MenuBar>
   <TextField fx:id="Name" prefHeight="25.0" prefWidth="560.0">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </VBox.margin></TextField>
   <TextField fx:id="Email">
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </VBox.margin>
   </TextField>
    <Button fx:id="button" onAction="#goToSecondScreen" prefHeight="64.0" prefWidth="239.0" text="Send">
      <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </padding>
      <VBox.margin>
         <Insets bottom="20.0" left="20.0" right="20.0" />
      </VBox.margin></Button>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
